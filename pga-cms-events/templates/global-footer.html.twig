<div class="footer-wrapper">
  <div class="main-menu">
    <ul>
      {% for menu in main_menu %}
        <li><a href="{{ menu.url }}">{{ menu.title }}</a></li>
    {% endfor %}
      </ul>
    </div>

    <div class="footer-links">
      <ul>
      {% for links in footer_link %}
          <li><a href="{{ links.url }}">{{ links.title }}</a></li>
    {% endfor %}
        </ul>
      </div>

</div>